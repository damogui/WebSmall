.place_info {
    padding: 0 5px;
    z-index: 10;
    font-size: 20px;
    color: #757a7e;
    line-height: 20px;
    font-family: Menlo, Monaco, Consolas, "Courier New", monospace;
}

.location {
    /* min-height: 20rpx; */
    /* padding: 10rpx;*/
    margin: 25rpx;
    background-color: #f5f5f5;
    border: 1px solid #e3e3e3;
    border-radius: 10rpx;
    -webkit-box-shadow: inset 0 1rpx 1rpx rgba(0, 0, 0, 0.05);
    box-shadow: inset 0 1rpx 1rpx rgba(0, 0, 0, 0.05);
    background-color: #fcf8e3;
    /* display: flex; */
}

.callphone {
    color: #FFFFFF;
    margin-top: 60rpx;
    text-align: center;
    line-height: 20rpx;
    /* width: 100%; */
    align-items: center;
    /*垂直居中*/
    justify-content: center;
    /* background-color: #1AAD19; */
    display: inline-block;
    width: 100%;
    height: 40px;
    line-height: 40px;
    text-align: center;
    border: 1px solid #1AAD19;
    border-radius: 5px;
    background: #1AAD19;
    cursor: pointer;
}