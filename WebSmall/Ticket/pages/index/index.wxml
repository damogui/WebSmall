      <image class="userinfo-avatar" src="/resources/set.png" background-size="cover"  bindtap="toSet"></image>
  </view>
  <view class="userinfo">
    <view class="scan" bindtap="scan">
      <image class="userinfo-avatar" src="/resources/sao.png" background-size="cover"></image>
